{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
   ],
   "target": "es5",
   "module": "commonjs",
   "moduleResolution": "node",
    "composite": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strictPropertyInitialization": false,
     /* Decorators */
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true,
     "outDir": "dist"
  }
}

[
   {
      "name": "default",
      "type": "sqlite",
      "database": "database.sqlite",
      "synchronize": true,
      "logging": false,
      "entities": [
         "entities/*.ts"
      ],
      "migrations": [
         "src/migration/**/*.ts"
      ],
      "subscribers": [
         "src/subscriber/**/*.ts"
      ],
      "cli": {
         "entitiesDir": "src/entity",
         "migrationsDir": "src/migration",
         "subscribersDir": "src/subscriber"
      }
   }
]